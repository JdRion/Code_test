   "source": [
    "# 1이 될 때 까지 반복 둘 중 하나\n",
    "# 두 번째는 N % K == 0 일 때만\n",
    "# 1. N - 1\n",
    "# 2. N / K"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 1,
   "metadata": {},
   "outputs": [],
   "source": [
    "# 나뉘어질 수 있을 때까지 빼기"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "2\n"
     ]
    }
   ],
   "source": [
    "N,K = map(int,input().split())\n",
    "\n",
    "count = 0\n",
    "while(N != 1):\n",
    "    if N % K == 0:\n",
    "        N = N/K\n",
    "    else:\n",
    "        N -= 1\n",
    "    count += 1\n",
    "\n",
    "print(count)"
   ]
  },
